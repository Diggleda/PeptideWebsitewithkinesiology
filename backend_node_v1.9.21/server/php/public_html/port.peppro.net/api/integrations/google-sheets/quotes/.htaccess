# Ensure this directory is served by Apache/PHP, not Passenger/Node
PassengerEnabled off
Options -MultiViews

<IfModule mod_rewrite.c>
  RewriteEngine On
  # Do not rewrite this request elsewhere
  RewriteRule ^quotes\.php$ - [L]
</IfModule>

# Common cPanel PHP handler (adjust if your account uses a different version)
<IfModule mod_mime.c>
  AddType application/x-httpd-ea-php82 .php
</IfModule>

